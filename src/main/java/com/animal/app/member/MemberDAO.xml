<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <!-- namespace의 이름은 사용하려는 DAO의 풀패키지명과 동일하게 작성 -->
  <mapper namespace="com.animal.app.member.MemeberDAO">
  
  
  <!-- Join(회원가입) -->
  <insert id="join" parameterType="MemberVO">
  	INSERT INTO MEMBER
  	VALUES (#{memberNo}, #{memberId}, #{memberPassword}, #{memberPhone}, #{memberEmail}, #{memberName}, #{memberRole} )
  </insert>
  
  
